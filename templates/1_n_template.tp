<sms> 
    <username><%= username %></username> 
    <password><%= password %></password>
    <header><%= header %></header> 
    <validity>2880</validity> 
    <message>
        <gsm> 
            <% for(var i=0; i<nums.length; i++) {%>
                <no><%= nums[i] %></no>
            <% } %>
        </gsm> 
        <msg><![CDATA[<%= message %>]]></msg>
    </message> 
</sms>